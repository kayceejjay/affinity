<p id="notice"><%= notice %></p>

<h1>Businesses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Category</th>
      <th>Fee</th>
      <th>Website</th>
      <th>Leader name</th>
      <th>Rep phone</th>
      <th>Rep email</th>
      <th>Service product</th>
      <th>Receive email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.name %></td>
        <td><%= business.address1 %></td>
        <td><%= business.address2 %></td>
        <td><%= business.city %></td>
        <td><%= business.state %></td>
        <td><%= business.zip %></td>
        <td><%= business.phone %></td>
        <td><%= business.email %></td>
        <td><%= business.category %></td>
        <td><%= business.fee %></td>
        <td><%= business.website %></td>
        <td><%= business.leader_name %></td>
        <td><%= business.rep_phone %></td>
        <td><%= business.rep_email %></td>
        <td><%= business.service_product %></td>
        <td><%= business.receive_email %></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_path %>
